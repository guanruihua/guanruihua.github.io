{
  "name": "0file-system",
  "version": "0.1.0",
  "description": "文件管理",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "unpkg": "lib/",
  "jsdelivr": "lib/",
  "types": "lib/index.d.ts",
  "author": "ruihuag",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "commitlint": "commitlint -e",
    "build2": "npx rollup -c --extend rollup.config.ts",
    "build": "npx rollup -c ./config/rollup.build.ts",
    "dev": "ts-node-dev --respawn --transpile-only ./test/index.test.ts",
    "test:old": "ts-node ./test/index.test.ts",
    "test:run": "ts-node ./test/index.ts",
    "test:remove": "ts-node ./src/remove/__test__/index.ts",
    "test:write": "ts-node ./src/write/__test__/index.ts",
    "test": "ts-node-dev --respawn --transpile-only ./test/index.ts",
    "restart": "rimraf dist && npm run build && npm start",
    "start": "node ./dist/index.js",
    "eslint": "eslint . --ext .ts --fix",
    "pub": "npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/abandonjs/0file-system.git"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "index.js"
  ],
  "bugs": {
    "url": "https://github.com/abandonjs/0file-system/issues"
  },
  "keywords": [
    "typescript",
    "fs"
  ],
  "homepage": "https://github.com/abandonjs/0file-system",
  "devDependencies": {
    "0log": "^0.0.1",
    "@babel/core": "^7.20.5",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.5",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/bluebird": "^3.5.36",
    "@types/node": "^18.11.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "abandonjs": "^2.6.0",
    "check-it-type": "0.4.2",
    "csvtojson": "^2.0.10",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "lint-staged": "^12.0.3",
    "node-notifier": "^10.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.8.4",
    "unit-testing-js": "0.5.0"
  }
}